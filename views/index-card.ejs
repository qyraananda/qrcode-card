<!DOCTYPE html>
<html>

<head>
    <title>
        <%= title %>
    </title>
    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js"></script>

    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>

<body>
    <div class="slide-container">
        <div class="wrapper">
            <div class="clash-card giant">
                <div class="clash-card__image clash-card__image--giant">
                    <img src="../images/logo.png" alt="logo" />
                </div>
                <%if(data.length>0){%>
                    <% data.forEach((rows,index)=>{%>
                        <div class="clash-card__unit-name">
                            <%=rows.name%>
                        </div>
                        <div class="clash-card__level clash-card__level--giant">
                            <%=rows.job%>
                        </div>
                        <div class="clash-card__unit-description">
                            <div class="one-third">
                                <div class="stat-value"><i class="fa fa-phone-square"></i>&nbsp;
                                    <%=rows.user_mobile%>
                                </div>
                                <div class="stat"><sup>CELL</sup></div>
                            </div>

                            <div class="one-third">
                                <div class="stat-value"><i class="fa fa-envelope"></i>&nbsp;
                                    <%=rows.email%>
                                </div>
                                <div class="stat"><sup>EMAIL</sup></div>
                            </div>

                            <div class="one-third no-border">
                                <div class="stat-value"><i class="fa fa-home"></i> <a href="https://egogohub.com">https://egogohub.com</a></div>
                                <div class="stat"><sup>WEB</sup></div>
                            </div>
                        </div>
                        <%})}%>
                            <div class="clash-card__unit-stats clash-card__unit-stats--giant clearfix">
                                <div class="row">
                                    <p>Pantai Indah Selatan <br>Ruko Elang Laut Boulevard Blok F No.1-2<br>Pantai Indah Kapuk<br>Jakarta Utara 14470<br>DKI Jakarta<br>Indonesia</p>
                                </div>
                            </div>

            </div>
            <!-- end clash-card giant-->
        </div>
        <!-- end wrapper -->
    </div>
    <!-- end container -->
</body>

</html>